	"check __name__": {
		"prefix": "m_",
		"body": [
			"if __name__ == \"__main__\":",
			"    $0",
		]
	},
	"prop": {
		"prefix": "prop",
		"body": [
			"@property",
			"def ${1:propname}(self):",
			"    return self._${1:propname}",
			"$0"
		]
	},
	"props": {
		"prefix": "props",
		"body": [
			"@property",
			"def $1(self):",
			"    return self._$1",
            "",
			"@$1.setter",
			"def $1(self, value):",
			"    self._$1 = value",
			"$0"
		]
	},
	"print object": {
		"prefix": "p_",
		"body": [
			"print(\"${expr}: {}\".format(${expr}))",
			"$0"
		]
	},
	"print object newline": {
		"prefix": "p+",
		"body": [
			"print(\"${expr}: {}\\n\".format($expr))",
			"$0"
		]
	},

	"print line": {
		"prefix": "p-",
		"body": [
			"print('-' * 60)",
			"$0"
		]
	},
